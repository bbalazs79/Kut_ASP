@model List<Kutatas_core.Models.Food>

@{
    ViewData["Title"] = "Food";
}

<table>
    <thead>
        <tr>Name</tr>
        <tr>Price</tr>
        <tr>Image</tr>
    </thead>
    <tbody>
        @foreach (Food food in this.Model)
        {
            <tr>
                <td>@food.Name</td>
                <td>@food.Price</td>
                <td>TODO</td>
            </tr>
        }
    </tbody>
</table>


